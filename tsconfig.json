{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./ts",
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "built",
		"removeComments": false,
		"sourceMap": true,
		"target": "es2020",
	},
	"include": ["ts"],
	"exclude": []
}